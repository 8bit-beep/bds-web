{
  "name": "@bds-web/themes",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/next/index.js"
    },
    "./theme.tailwind.css": "./dist/theme.tailwind.css",
    "./init": {
      "types": "./dist/init-theme.iife.d.ts",
      "import": "./dist/init-theme.iife.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node ./scripts/copy-assets.mjs",
    "prepack": "pnpm run build",
    "typecheck": "tsc -b"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "next": ">=16",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1"
  }
}

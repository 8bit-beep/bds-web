{
  "name": "@bds-web/shapes",
  "version": "1.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tokens.tailwind.css": "./dist/tokens.tailwind.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node ./scripts/copy-assets.mjs",
    "prepack": "pnpm run build",
    "typecheck": "tsc -b"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  }
}
